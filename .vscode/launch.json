{
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Run gdtk",
            "cargo": {
                "args": [
                    "build",
                ],
                "filter": {
                    "name": "gdtk",
                    "kind": "bin",
                }
            },
            "args": ["${input:subcommand}", "${input:subsubcommand}"]
        }
    ],
    "inputs": [
        {
            "id": "subcommand",
            "description": "Enter gdtk CLI args",
            "default": "",
            "type": "promptString"
        },
        {
            "id": "subsubcommand",
            "description": "Enter gdtk CLI args",
            "default": "",
            "type": "promptString"
        }
    ]
}
