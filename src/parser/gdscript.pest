identifier = {
	ALPHABETIC+
	~ ASCII_ALPHANUMERIC*
}

comment = {
    "#"
	~ (!"\n" ~ ANY)*
}

annotation = {
	"@"
	~ identifier
	~ ( !("\n" | " ") ~ "()"* )
	~ ( !("\n" | " ") ~ ANY)*
}

statement = _{
	comment
	| annotation
}

file = _{
	SOI
	~ "\n"*
	~ (statement* ~ "\n"+)*
	~ statement?
	~ EOI
}
