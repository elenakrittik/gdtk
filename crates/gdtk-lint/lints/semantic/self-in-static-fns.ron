LintQuery (
    identifier: "E001",
    query: r#"{
        Statement {
            ... on IdentifierValue {
                inner @filter(op: "!=", value: ["$self_ident"]) @output
            }
            ... on BinaryExprValue {
                left @recurse(depth: 10)
                right @recurse(depth: 10)
            }
        }
    }"#,
    args: {
        "self_ident": String("self"),
    },
)
